{
  "mcpProxy": {
    "name": "x-forge MCP Proxy",
    "version": "1.0.0",
    "type": "stdio",
    "hierarchyPath": "/home/x-forge/.claude/lazy-mcp/hierarchy",
    "options": {
      "lazyLoad": true,
      "preloadAll": true
    }
  },
  "mcpServers": {
